{
  "version": 2,
  "functions": {
    "api/server.js": {
      "runtime": "@vercel/node@5.3.22"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "api/server.js" },
    { "src": "/js/(.*)", "dest": "/js/$1" },
    { "src": "/css/(.*)", "dest": "/css/$1" },
    { "src": "/img/(.*)", "dest": "/img/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
