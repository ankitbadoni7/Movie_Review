{
  "version": 2,
  "functions": {
    "api/server.js": {
      "runtime": "@vercel/node@5.3.22"
    }
  },
  "routes": [
     { "src": "/api/(.*)", "dest": "api/server.js" },

    { "src": "/js/(.*)", "dest": "/js/$1" },
    { "src": "/css/(.*)", "dest": "/css/$1" },
    { "src": "/img/(.*)", "dest": "/img/$1" },

    { "src": "/index.html", "dest": "/index.html" },
    { "src": "/movies.html", "dest": "/movies.html" },
    { "src": "/report.html", "dest": "/report.html" },
    { "src": "/movie_review.html", "dest": "/movie_review.html" },
    { "src": "/series.html", "dest": "/series.html" },
    { "src": "/about.html", "dest": "/about.html" },


    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
